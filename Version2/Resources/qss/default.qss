QTableView{  
    color: rgb(0, 0, 0);  
    border: 0;          /*边框颜色*/  
    gridline-color:white;             /*grid线颜色*/  
    background-color: white;  
    alternate-background-color: white; /*行交替颜色*/  
    selection-background-color: white; /*选中行背景颜色*/  
}  
  
QTableView::item:!alternate:!selected{  
    background-color: rgb(220, 220, 220);    /*交替行的另一颜色*/  
}  

QPushButton#HostCreate,QPushButton#SourceCodeCreate, QPushButton#BuildRuleCreate, QPushButton#DlgSelectHostOk,QPushButton#DlgSelectResourceSourceCodeOk,QPushButton#DlgSelectBuildRulePlatformOk{
	background-color: #007acc; 
	border:1px solid #007acc; 
	font-family: "verdana","tahoma","微软雅黑","宋体","Arial Narrow";
	font-size: 14px;
	height: 30px;
	width: 100px;
	color: white;
	border-radius: 2px;
}

QPushButton#HostCreate:hover, QPushButton#SourceCodeCreate:hover, QPushButton#BuildRuleCreate:hover, QPushButton#DlgSelectHostOk:hover, QPushButton#DlgSelectResourceSourceCodeOk:hover, QPushButton#DlgSelectBuildRulePlatformOk:hover{  
