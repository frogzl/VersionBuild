QTabWidget{
	border:0;
}
QToolButton{  
	background-color: #4e4d4d; 
	image: url(:Resources/png/appbar.control.down.png);  
	border:0;
} 
 
QToolButton:hover{  /*鼠标放上后*/  
	background-color: #007acc; 
	image: url(:Resources/png/appbar.control.down.png);  	
	border:0;
}  
QToolButton:pressed{ /*按下按钮后*/  
	background-color: #4e4d4d; 
	image: url(:Resources/png/appbar.control.down.png);  	
	border:0;
}  
QToolButton:checked{    /*选中后*/  
	background-color: #007acc; 
	image: url(:Resources/png/appbar.control.down.png);  
	border:0;
}  

QTabWidget::pane{
	border:none;
	background:white;
}
QTabWidget::tab-bar{
    alignment:left;
	border-width:0px;
}
QTabBar::tab{
	background:#3b3b3b;
	color:white;
	min-width:74px;
	min-height:10px;
}
QTabBar::tab:hover{
	background:#007acc;
}
QTabBar::tab:selected{
	border: 1px solid #007acc;
	background:#007acc;
	color:white;
}

/* === QMenu === */
QMenu {
    background-color: #4e4d4d;
    border: 1px solid gray;
    color: white;
    padding: 1px;
}

QMenu::item {
    padding: 2px 25px 2px 20px;
    border: 1px solid transparent;
	color: white;
}

QMenu::item:disabled {
    color: #666666;
}

QMenu::item:selected {
    border-color: #007acc;
    background: #007acc;
}

QMenu::icon:checked {

}

QMenu::separator {
    height: 1px;
    background: #222222;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 1px;
    margin-bottom: 1px;
}

QMenu::indicator {
    width: 13px;
    height: 13px;
}

/* === QMenuBar === */
QMenuBar {
    background-color: white;
    color: black;
	border-bottom: 1px solid #4e4d4d;
}

QMenuBar::item {
    background: transparent;
}

QMenuBar::item:disabled {
    color: gray;
}

QMenuBar::item:selected {
	background: #007acc;
	color: white;
}

QMenuBar::item:pressed {
    background: #4e4d4d;
    color: white;
}

